{
    "name": "sslah/location-finder",
    "description": "ðŸš€ Laravel Location Finder - Pure API Service for geocoding with OpenStreetMap/Nominatim. Framework-agnostic, no UI dependencies. Perfect for Turkish addresses!",
    "keywords": ["laravel", "location-finder", "geocoding", "api-service", "openstreetmap", "nominatim", "address-search", "turkey", "framework-agnostic"],
    "license": "MIT",
    "type": "library",
    "homepage": "https://github.com/ahmetsuslu/location-finder",
    "support": {
        "issues": "https://github.com/ahmetsuslu/location-finder/issues",
        "source": "https://github.com/ahmetsuslu/location-finder"
    },
    "authors": [
        {
            "name": "Sslah",
            "email": "mail@ahmetsuslu.com",
            "homepage": "https://github.com/ahmetsuslu",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.1",
        "laravel/framework": "^10.0|^11.0|^12.0",
        "guzzlehttp/guzzle": "^7.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.0",
        "orchestra/testbench": "^8.0|^9.0"
    },
    "autoload": {
        "psr-4": {
            "Sslah\\LocationFinder\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Sslah\\LocationFinder\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Sslah\\LocationFinder\\LocationFinderServiceProvider"
            ]
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "config": {
        "sort-packages": true
    }
}
