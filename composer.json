{
    "name": "sslah/location-finder",
    "description": "Laravel location-finder package",
    "keywords": ["laravel", "location-finder", "package"],
    "license": "MIT",
    "type": "library",
    "homepage": "https://github.com/ahmetsuslu/location-finder",
    "support": {
        "issues": "https://github.com/ahmetsuslu/location-finder/issues",
        "source": "https://github.com/ahmetsuslu/location-finder"
    },
    "authors": [
        {
            "name": "Sslah",
            "email": "mail@ahmetsuslu.com",
            "homepage": "https://github.com/ahmetsuslu",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.1",
        "laravel/framework": "^10.0^^|^11.0^^|^12.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.0",
        "orchestra/testbench": "^8.0^^|^9.0"
    },
    "autoload": {
        "psr-4": {
            "Sslah\\location-finder\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Sslah\\location-finder\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Sslah\\location-finder\\location-finderServiceProvider"
            ]
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "config": {
        "sort-packages": true
    }
}
